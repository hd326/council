        "tailwindcss": "^1.1.2",
        "vue": "^2.6.10"
    },
    "dependencies": {
        "at.js": "^1.5.4",
        "bootstrap": "^4.3.1",
        "bootstrap-vue": "^2.0.0-rc.28",
        "jquery.caret": "^0.3.1",
